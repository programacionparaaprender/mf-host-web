{
  "name": "mf-host-auth0",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "run:all": "node node_modules/@angular-architects/module-federation/src/server/mf-dev-server.js",
    "compodoc2": "npx compodoc -p tsconfig.app.json -d documentation --serve --open",
    "compodoc": "npx compodoc -p tsconfig.app.json --output src/docs --serve --open",
    "compodoc:build": "npx compodoc -p tsconfig.app.json -d documentation",
    "compodoc:material": "npx compodoc -p tsconfig.app.json -d documentation --theme material --serve --open",
    "compodoc:clean": "rm -rf documentation"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.12",
    "@angular/compiler": "^20.3.12",
    "@angular/core": "^20.3.12",
    "@angular/forms": "^20.3.12",
    "@angular/material": "^20.2.13",
    "@angular/platform-browser": "^20.3.0",
    "@angular/router": "^20.3.0",
    "@ngrx/effects": "^20.1.0",
    "@ngrx/store": "^20.1.0",
    "@ngrx/store-devtools": "^20.1.0",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "bootstrap": "^5.3.3",
    "jquery": "^3.7.1",
    "ngrx-store-localstorage": "^20.0.0",
    "popper.js": "^1.16.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-architects/module-federation": "^20.0.0",
    "@angular-devkit/build-angular": "^20.3.10",
    "@angular/build": "^20.3.10",
    "@angular/cli": "^20.3.10",
    "@angular/compiler-cli": "^20.3.0",
    "@auth0/auth0-angular": "^2.2.3",
    "@compodoc/compodoc": "^1.1.32",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ngx-build-plus": "^20.0.0",
    "typescript": "~5.9.2"
  }
}
